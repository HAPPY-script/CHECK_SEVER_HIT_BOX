local placeId = game.PlaceId

if placeId == 2753915549 then
    -- Sea 1
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HAPPY-script/SELECT_HIT_BOX_NPC/refs/heads/main/SELECT_HIT_BOX_PLAYER_NPC"))()
elseif placeId == 4442272183 then
    -- Sea 2
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HAPPY-script/SELECT_HIT_BOX_NPC/refs/heads/main/SELECT_HIT_BOX_PLAYER_NPC"))()
elseif placeId == 7449423635 then
    -- Sea 3
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HAPPY-script/SELECT_HIT_BOX_NPC/refs/heads/main/SELECT_HIT_BOX_PLAYER_NPC"))()
else
    -- Nếu không phải cả 3 Sea trên, chạy script ngoại lệ
    loadstring(game:HttpGet("https://raw.githubusercontent.com/HAPPY-script/HIT_BOX/refs/heads/main/HIT_BOX"))()
end
